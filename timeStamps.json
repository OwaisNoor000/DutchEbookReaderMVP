[{"time": 0, "type": "sentence", "start": 27, "end": 52, "value": "Een brief aan Sinterklaas"}, {"time": 3037, "type": "sentence", "start": 57, "end": 96, "value": "Ik heb Sinterklaas gezien,\u2019 zegt Jip."}, {"time": 6900, "type": "sentence", "start": 101, "end": 110, "value": "Waar dan?"}, {"time": 8145, "type": "sentence", "start": 114, "end": 129, "value": "vraagt Janneke."}, {"time": 10248, "type": "sentence", "start": 134, "end": 159, "value": "In de krant,\u2019 zegt Jip."}, {"time": 12999, "type": "sentence", "start": 164, "end": 196, "value": "O, in de krant,\u2019 zegt Janneke."}, {"time": 16190, "type": "sentence", "start": 200, "end": 234, "value": "Ik heb hem ook in de\nkrant gezien."}, {"time": 19205, "type": "sentence", "start": 235, "end": 259, "value": "Maar hij is er nog niet."}, {"time": 21844, "type": "sentence", "start": 267, "end": 331, "value": "Ja, maar,\u2019 zegt Jip, \u2018ik wil zo graag mijn schoentje\nzetten."}, {"time": 27294, "type": "sentence", "start": 339, "end": 362, "value": "He ja,\u2019 zegt Janneke."}, {"time": 29817, "type": "sentence", "start": 366, "end": 389, "value": "Hier bij jou thuis Jip."}, {"time": 32158, "type": "sentence", "start": 390, "end": 431, "value": "Mijn\nmoeder zegt dat het nog te vroeg is."}, {"time": 36125, "type": "sentence", "start": 436, "end": 494, "value": "Ze halen allebei een schoen en zetten die naast de\nkachel."}, {"time": 41477, "type": "sentence", "start": 499, "end": 527, "value": "Je ziet het haast niet, h\u00e9?"}, {"time": 44197, "type": "sentence", "start": 531, "end": 540, "value": "zegt Jip."}, {"time": 45869, "type": "sentence", "start": 545, "end": 566, "value": "Nee,\u2019 zegt Janneke."}, {"time": 48036, "type": "sentence", "start": 570, "end": 608, "value": "Sinterklaas zal er geen erg in\nhebben."}, {"time": 51863, "type": "sentence", "start": 609, "end": 652, "value": "Zullen we nog meer schoenen ernaast zetten?"}, {"time": 55584, "type": "sentence", "start": 660, "end": 680, "value": "H\u00e9 ja,\u2019 zegt Jip."}, {"time": 57855, "type": "sentence", "start": 684, "end": 721, "value": "Haal jij ook maar meer, bij je thuis."}, {"time": 61611, "type": "sentence", "start": 726, "end": 778, "value": "Dan hebben ze eindelijk een heleboel schoenen staan."}, {"time": 65971, "type": "sentence", "start": 783, "end": 813, "value": "En nu zingen,\u2019 zegt Janneke."}, {"time": 68771, "type": "sentence", "start": 817, "end": 827, "value": "Heel hard."}, {"time": 70552, "type": "sentence", "start": 832, "end": 858, "value": "Ze zingen ontzettend hard."}, {"time": 73402, "type": "sentence", "start": 860, "end": 912, "value": "Maar dan komt Jips moeder en zegt: \u2018Wat is dat nu?"}, {"time": 77859, "type": "sentence", "start": 913, "end": 945, "value": "Sinterklaas is er nog lang niet."}, {"time": 80941, "type": "sentence", "start": 946, "end": 972, "value": "Jullie zijn veel te vroeg."}, {"time": 83276, "type": "sentence", "start": 973, "end": 1025, "value": "Maar je mag wel een brief aan Sinterklaas schrijven."}, {"time": 87846, "type": "sentence", "start": 1026, "end": 1034, "value": "Dat\nkan."}, {"time": 89181, "type": "sentence", "start": 1035, "end": 1048, "value": "Doe dat maar."}, {"time": 90929, "type": "sentence", "start": 1053, "end": 1082, "value": "Ze schrijven een mooie brief."}, {"time": 93762, "type": "sentence", "start": 1083, "end": 1109, "value": "Jip tekent een vrachtauto."}, {"time": 96580, "type": "sentence", "start": 1110, "end": 1142, "value": "En Janneke tekent een fornuisje."}, {"time": 99607, "type": "sentence", "start": 1143, "end": 1181, "value": "Want dat willen ze het\nliefste hebben."}]