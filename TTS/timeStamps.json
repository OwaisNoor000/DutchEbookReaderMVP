[{"time": 0, "type": "sentence", "start": 27, "end": 95, "value": "Een brief aan Sinterklaas \u2018Ik heb Sinterklaas gezien,\u2019 zegt Jip."}, {"time": 5456, "type": "sentence", "start": 99, "end": 108, "value": "Waar dan?"}, {"time": 6701, "type": "sentence", "start": 112, "end": 127, "value": "vraagt Janneke."}, {"time": 8553, "type": "sentence", "start": 131, "end": 156, "value": "In de krant,\u2019 zegt Jip."}, {"time": 11055, "type": "sentence", "start": 160, "end": 192, "value": "O, in de krant,\u2019 zegt Janneke."}, {"time": 14246, "type": "sentence", "start": 196, "end": 230, "value": "Ik heb hem ook in de krant gezien."}, {"time": 17261, "type": "sentence", "start": 231, "end": 255, "value": "Maar hij is er nog niet."}, {"time": 19650, "type": "sentence", "start": 262, "end": 326, "value": "Ja, maar,\u2019 zegt Jip, \u2018ik wil zo graag mijn schoentje zetten."}, {"time": 24850, "type": "sentence", "start": 333, "end": 356, "value": "He ja,\u2019 zegt Janneke."}, {"time": 27373, "type": "sentence", "start": 360, "end": 383, "value": "Hier bij jou thuis Jip."}, {"time": 29713, "type": "sentence", "start": 384, "end": 425, "value": "Mijn moeder zegt dat het nog te vroeg is."}, {"time": 33431, "type": "sentence", "start": 429, "end": 487, "value": "Ze halen allebei een schoen en zetten die naast de kachel."}, {"time": 38533, "type": "sentence", "start": 491, "end": 519, "value": "Je ziet het haast niet, h\u00e9?"}, {"time": 41252, "type": "sentence", "start": 523, "end": 532, "value": "zegt Jip."}, {"time": 42674, "type": "sentence", "start": 536, "end": 557, "value": "Nee,\u2019 zegt Janneke."}, {"time": 44841, "type": "sentence", "start": 561, "end": 599, "value": "Sinterklaas zal er geen erg in hebben."}, {"time": 48669, "type": "sentence", "start": 600, "end": 643, "value": "Zullen we nog meer schoenen ernaast zetten?"}, {"time": 52139, "type": "sentence", "start": 650, "end": 670, "value": "H\u00e9 ja,\u2019 zegt Jip."}, {"time": 54410, "type": "sentence", "start": 674, "end": 711, "value": "Haal jij ook maar meer, bij je thuis."}, {"time": 57916, "type": "sentence", "start": 715, "end": 767, "value": "Dan hebben ze eindelijk een heleboel schoenen staan."}, {"time": 62027, "type": "sentence", "start": 771, "end": 801, "value": "En nu zingen,\u2019 zegt Janneke."}, {"time": 64827, "type": "sentence", "start": 805, "end": 815, "value": "Heel hard."}, {"time": 66358, "type": "sentence", "start": 819, "end": 845, "value": "Ze zingen ontzettend hard."}, {"time": 68957, "type": "sentence", "start": 846, "end": 898, "value": "Maar dan komt Jips moeder en zegt: \u2018Wat is dat nu?"}, {"time": 73414, "type": "sentence", "start": 899, "end": 931, "value": "Sinterklaas is er nog lang niet."}, {"time": 76497, "type": "sentence", "start": 932, "end": 958, "value": "Jullie zijn veel te vroeg."}, {"time": 78832, "type": "sentence", "start": 959, "end": 1011, "value": "Maar je mag wel een brief aan Sinterklaas schrijven."}, {"time": 83402, "type": "sentence", "start": 1012, "end": 1020, "value": "Dat kan."}, {"time": 84736, "type": "sentence", "start": 1021, "end": 1034, "value": "Doe dat maar."}, {"time": 86235, "type": "sentence", "start": 1038, "end": 1067, "value": "Ze schrijven een mooie brief."}, {"time": 89067, "type": "sentence", "start": 1068, "end": 1094, "value": "Jip tekent een vrachtauto."}, {"time": 91885, "type": "sentence", "start": 1095, "end": 1127, "value": "En Janneke tekent een fornuisje."}, {"time": 94913, "type": "sentence", "start": 1128, "end": 1166, "value": "Want dat willen ze het liefste hebben."}]